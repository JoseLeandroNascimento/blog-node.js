<%- include("../../partials/header.ejs") %>

    <body>

        <%- include("../../partials/navbar.ejs") %>

            <div class="container">
                <hr>
                <h2>Categorias</h2>
                <a href="/admin/users/create" class="btn btn-success">
                    Criar novo usuário
                </a>
                <hr>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Email</th>
                            
                            <!-- <th>Ações</th> -->
                        </tr>
                    </thead>
                    <tbody>

                        <% usuarios.forEach(usuario=> { %>
                            <tr>
                                <td>
                                    <%= usuario.id %>
                                </td>
                                <td>
                                    <%= usuario.email %>
                                </td>
                               
                               
                            </tr>
                            <% }) %>
                    </tbody>
                </table>
            </div>

    </body>
    <%- include("../../partials/footer.ejs") %>